@section scripts
{
<script>
    $(document).ready(function () {
        $(".collapse-item").click(function() {
	        $(this).addClass("active");
        });

        $(".nav-item").click(function() {
            if(!(this).hasClass("active")) {
                $(".nav-item").addClass("active");   
            }
        });
        
    });
</script>
}